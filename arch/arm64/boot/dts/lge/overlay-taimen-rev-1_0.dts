/dts-v1/;
/plugin/;

#include "msm8998-taimen.dtsi"

&i2c_5 {
	status = "ok";
};

&pm8998_vadc {
	chan@83 {
		label = "vph_pwr";
		reg = <0x83>;
		qcom,decimation = <2>;
		qcom,pre-div-channel-scaling = <1>;
		qcom,calibration-type = "absolute";
		qcom,scale-function = <0>;
		qcom,hw-settle-time = <0>;
		qcom,fast-avg-setup = <0>;
	};
};

&spi_5 {
	status = "disabled";
};

&soc {
	qcom,sensor-information {
		qcom,sensor-information-31 {
			qcom,sensor-name = "bd_therm2";
		};
	};
};

&pmi8998_charger {
	goog,port-overheat-mitigation;
	goog,port-overheat-usb-port-tz-name = "usb_port_temp";
	goog,port-overheat-begin-temp = <600>;
	goog,port-overheat-end-temp = <500>;
	goog,port-overheat-work-interval = <1000>;
};

&pm8998_vadc {
	chan@51 {
		label = "bd_therm2";
	};
};

&pm8998_adc_tm {
	chan@51 {
		label = "bd_therm2";
	};
};
